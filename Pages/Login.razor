@page "/Login"

<div class="card">
    <div class="card-header bg-info">
        <h2>Login</h2>
    </div>
    <div class="card-body">
        <div class="form-group">
            <h5>User Name:</h5>
            <InputText @bind-Value="UserName"></InputText>
            <h5>Password:</h5>
            <InputText @bind-Value="Password"></InputText>
        </div>
        
    </div>

</div>

@code {
    string Password;
    string UserName;
}
